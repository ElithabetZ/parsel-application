<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet id="26-11-2023-transaction-user-constraint" author="Liza">
        <addForeignKeyConstraint
                baseTableName="transaction"
                baseColumnNames="id_sender"
                constraintName="FK_id_sender_TR"
                referencedTableName="user"
                referencedColumnNames="id"></addForeignKeyConstraint>
    </changeSet>

    <changeSet id="26-11-2023-transaction-order-constraint" author="Liza">
        <addForeignKeyConstraint
                baseTableName="transaction"
                baseColumnNames="id_order"
                constraintName="FK_id_order"
                referencedTableName="order"
                referencedColumnNames="id"></addForeignKeyConstraint>
    </changeSet>
</databaseChangeLog>