<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd">
    <changeSet id="26-11-2023-user-address-constraint" author="Liza">
        <addForeignKeyConstraint
                baseTableName="user"
                baseColumnNames="id_address"
                constraintName="FK_id_address"
                referencedTableName="address"
                referencedColumnNames="id"></addForeignKeyConstraint>
    </changeSet>

    <changeSet id="26-11-2023-user-contact-constraint" author="Liza">
        <addForeignKeyConstraint
                baseTableName="user"
                baseColumnNames="id_contact"
                constraintName="FK_id_contact"
                referencedTableName="contact"
                referencedColumnNames="id"></addForeignKeyConstraint>
    </changeSet>
</databaseChangeLog>